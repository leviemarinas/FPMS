extends ../../../templates/CSmaster

block content
    -var pic = Profiles[0].strFacultyImageUrl
        div.ui.s.adjusted.positioning.ping
            div.ui.container
                div.ui.ribbon.label.inverted.black.large
                    h3 Educational Attainment
                div.ui.segment.height.b-adjusted 
                    img.ui.image(src=`${pic}`, alt="/images/wireframe/image.png" height="200px" width="200px")
                    div.ui.container.height
                        div.ui.container
                            each Profile in Profiles
                                label.ui.ribbon.inverted.teal.large.label #{Profile.strEducLevelName}
                                table.ui.padded.green.table
                                    tbody
                                            tr
                                                td.six.wide 
                                                    b School 
                                                td= Profile.strEducAttainSchoolName
                                            tr 
                                                td  
                                                    b Education Attained
                                                td= Profile.strEducAttainName
                                            tr 
                                                td  
                                                    b Grad Year
                                                td= Profile.strGradYear